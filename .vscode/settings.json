{
  "latex-workshop.latex.recipes": [
    {
      "name": "build with externalized images",
      "tools": ["cleanstyles", "pdflatex", "pdflatex"]
    },
    {
      "name": "clean and build with externalized images",
      "tools": ["cleanfiles", "cleanstyles", "pdflatex", "pdflatex"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "cleanstyles",
      "command": "python3",
      "args": ["envsetup.py", "clean"],
      "env": {}
    },
    {
      "name": "cleanfiles",
      "command": "python3",
      "args": ["envsetup.py", "delete"],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    }
  ],
  "latex-workshop.latex.recipe.default": "build with externalized images",
  "latex-workshop.latex.autoBuild.interval": 5000,
  "latex-workshop.message.warning.show": false,
  "latex-workshop.message.error.show": false,
  "latex-workshop.view.outline.sections": [
    "part",
    "chapter",
    "section",
    "subsection",
    "subsubsection",
    "paragraph"
  ],
  "latex-workshop.latexindent.args": [
    "-c",
    "%DIR%/",
    "%TMPFILE%",
    "-y=defaultIndent: '%INDENT%'",
    "-m"
  ],
  "[latex]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": [
    "ADDV",
    "Almasi",
    "analyzed",
    "andi",
    "Antidependence",
    "auxlock",
    "bgtz",
    "BNEZ",
    "cleantikz",
    "Conficconi",
    "CUDA",
    "DADDIU",
    "DSUBIU",
    "freelist",
    "hlines",
    "Hyperblock",
    "latexpdf",
    "maskable",
    "MFLOPS",
    "MISD",
    "mispredict",
    "mispredicted",
    "mult",
    "nonmaskable",
    "pipelining",
    "restartability",
    "Sciuto",
    "setvlr",
    "SIMT",
    "SISD",
    "Speedup",
    "speedups",
    "strided",
    "stripmining",
    "Superblock",
    "tikz",
    "tikzit",
    "tikzstyle",
    "tikzstyles",
    "tikzsyles",
    "tikztemp",
    "Tomasulo",
    "vectorizable",
    "vfadd",
    "vfsgts",
    "VLIW",
    "VMIPS",
    "VMMAs",
    "writable",
    "writeback",
    "writebacks"
  ],
  "grammarly.selectors": [
    {
      "language": "latex",
      "scheme": "file",
      "pattern": "advanced-computer-architectures.tex"
    }
  ]
}
